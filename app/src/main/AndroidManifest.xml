<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gmail.kludgeworks.bluebirdboxmonitoring" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".GooglePlayServicesActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".InputActivity"
            android:label="@string/title_activity_input"
            android:parentActivityName=".GooglePlayServicesActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.gmail.kludgeworks.bluebirdboxmonitoring.GooglePlayServicesActivity" />
        </activity>

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>
